{
  "expo": {
    "name": "Mirow",
    "slug": "mirow",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/mirow-logo.png",
    "scheme": "mirow",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "backgroundColor": "#ffffff",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#ffffff",
      "bundleIdentifier": "com.mirow.mirow",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mirow.mirow",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/mirow-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mirow-color.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-system-ui",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/mirow-logo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "88d0f8cc-c6af-48a1-8bc4-5727b30fafc1"
      }
    }
  }
}
